        <Version>1.0.0</Version>
        <RootNamespace>NCANode</RootNamespace>
        <PackageTags>nca node wisk adata alldata</PackageTags>
        <Company>Wisk Telecom Solutions</Company>
        <Authors>Yevgeniy Cherdantsev</Authors>
        <PackageId>NCANode</PackageId>
        <Copyright>Yevgeniy Cherdantsev</Copyright>
